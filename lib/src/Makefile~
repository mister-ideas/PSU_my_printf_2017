##
## EPITECH PROJECT, 2017
## libmy.a
## File description:
## Makefile
##

SRC	=	my_put_nbr.c	\
		my_strlen.c	\
		my_putchar.c	\
		my_putstr.c	\
		sum_stdarg.c	\
		disp_stdarg.c	\

OBJ	=	$(SRC:.c=.o)

NAME	=	libmy.a

all:		$(NAME)

$(NAME):	$(OBJ)
		ar rc $(NAME) $(OBJ)
		mkdir -p ../my
		mv $(NAME) ../my/

clean:
		rm -f $(OBJ)

fclean: 	clean
		rm -f ../my/$(NAME)

re:		fclean all

.PHONY:		all clean fclean re
